<?xml version="1.0" encoding="utf-8"?>
<com.yunbao.common.custom.MyLinearLayout7 xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginEnd="@dimen/dp5"
    android:background="@color/white"
    android:orientation="vertical"
    app:mll4_count="2"
    app:mll4_count_dp="30">

    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">

        <TextView
            android:id="@+id/tv_item_home_game_title"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="5dp"
            android:layout_marginTop="6dp"
            android:layout_toStartOf="@+id/tv_game_status"
            android:ellipsize="end"
            android:lines="1"
            android:textColor="#ff2b2626"
            android:textSize="13sp"
            tools:ignore="MissingConstraints"
            tools:text="英雄联盟" />

        <TextView
            android:id="@+id/tv_game_status"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:gravity="center"
            android:paddingStart="5dp"
            android:paddingTop="2.7dp"
            android:paddingEnd="5.6dp"
            android:paddingBottom="2.7dp"
            android:textColor="@color/white"
            android:textSize="10sp"
            tools:text="比赛中" />

    </RelativeLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/dp_10"
        android:layout_marginTop="13dp"
        android:layout_marginEnd="@dimen/dp_10"
        android:layout_marginBottom="20dp"
        android:gravity="center"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/iv_left"
            android:layout_width="26dp"
            android:layout_height="26dp"
            android:src="@mipmap/icon_default_logo" />

        <TextView
            android:id="@+id/tv_game_result"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="12dp"
            android:layout_marginEnd="12dp"
            android:gravity="center"
            android:minWidth="70dp"
            android:textColor="#FF5116"
            android:textSize="13sp"
            tools:text="23 : 12" />

        <ImageView
            android:id="@+id/iv_right"
            android:layout_width="26dp"
            android:layout_height="26dp"
            android:src="@mipmap/icon_default_logo" />
    </LinearLayout>

    <TextView
        android:id="@+id/tv_item_home_game_hint"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_marginBottom="10dp"
        android:ellipsize="end"
        android:lines="1"
        android:textColor="@color/black3"
        android:textSize="11sp"
        tools:text="2020LOL世界s10冠军赛" />
</com.yunbao.common.custom.MyLinearLayout7>

