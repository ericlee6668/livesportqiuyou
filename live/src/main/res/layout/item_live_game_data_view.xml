<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:gravity="center|start"
    android:minHeight="24dp"
    android:paddingTop="@dimen/dp5"
    android:paddingBottom="@dimen/dp5">

    <LinearLayout
        android:id="@+id/ll_start"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:gravity="center|start"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/iv_team_logo"
            android:layout_width="22dp"
            android:layout_height="22dp"
            android:src="@mipmap/icon_default_logo" />

        <TextView
            android:id="@+id/tv_name"
            android:layout_width="@dimen/dp40"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/dp5"
            android:ellipsize="end"
            android:maxLines="2"
            android:textColor="#1890FF"
            android:textSize="12sp"
            tools:text="hjakhdasdsdashj" />
    </LinearLayout>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:layout_toStartOf="@+id/ll_end"
        android:layout_toEndOf="@+id/ll_start"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/tv_num"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_weight="1"
            android:ellipsize="end"
            android:gravity="end"
            android:maxLines="2"
            android:textColor="@color/black3"
            android:textSize="12sp"
            tools:text="15.567K" />

        <TextView
            android:id="@+id/tv_game_data"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_weight="1"
            android:ellipsize="end"
            android:gravity="end"
            android:maxLines="2"
            android:textColor="@color/black3"
            android:textSize="12sp"
            tools:text="17/4/15" />
    </LinearLayout>


    <LinearLayout
        android:id="@+id/ll_end"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_centerVertical="true"
        android:gravity="end">

        <ImageView
            android:id="@+id/iv_1"
            android:layout_width="22dp"
            android:layout_height="22dp"
            android:layout_marginStart="@dimen/dp5"
            android:src="@mipmap/icon_lol_default" />

        <ImageView
            android:id="@+id/iv_2"
            android:layout_width="22dp"
            android:layout_height="22dp"
            android:layout_marginStart="@dimen/dp2"
            tools:src="@mipmap/icon_lol_default" />

        <ImageView
            android:id="@+id/iv_3"
            android:layout_width="22dp"
            android:layout_height="22dp"
            android:layout_marginStart="@dimen/dp2"
            android:src="@mipmap/icon_lol_default" />

        <ImageView
            android:id="@+id/iv_4"
            android:layout_width="22dp"
            android:layout_height="22dp"
            android:layout_marginStart="@dimen/dp2"
            android:src="@mipmap/icon_lol_default" />

        <ImageView
            android:id="@+id/iv_5"
            android:layout_width="22dp"
            android:layout_height="22dp"
            android:layout_marginStart="@dimen/dp2"
            android:src="@mipmap/icon_lol_default" />

        <ImageView
            android:id="@+id/iv_6"
            android:layout_width="22dp"
            android:layout_height="22dp"
            android:layout_marginStart="@dimen/dp2"
            android:src="@mipmap/icon_lol_default" />

        <ImageView
            android:id="@+id/iv_7"
            android:layout_width="22dp"
            android:layout_height="22dp"
            android:layout_marginStart="@dimen/dp2"
            android:src="@mipmap/icon_lol_default" />
    </LinearLayout>


</RelativeLayout>